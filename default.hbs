        var l2 = document.createElement('link'); l2.rel = 'stylesheet'; l2.href = '//fonts.googleapis.com/icon?family=Material+Icons';
        h.parentNode.insertBefore(l2, h);
        
        var l3 = document.createElement('link'); l3.rel = 'stylesheet'; l3.href = '//fonts.googleapis.com/css?family=Roboto:300,400,500,700';
        h.parentNode.insertBefore(l3, h);
        
        // var l4 = document.createElement('link'); l4.rel = 'stylesheet'; l4.href = '/assets/dist/all.min.css';
        // h.parentNode.insertBefore(l4, h);
        
